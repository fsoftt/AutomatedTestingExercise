{
  "Logging": {
    "MinimumLevel": "Debug"
  },
  "test": "testValue",
  "Url": "https://www.epam.com/",
  "CookiesElementBy": "div#onetrust-consent-sdk #onetrust-accept-btn-handler",
  "DownloadDirectory": "Downloads",
  "SearchForPositionBasedOnCriteria": {
    "AboutLinkText": "About",
    "CareersLinkText": "Careers",
    "InsightsLinkText": "Insights",
    "KeywordsId": "new_form_job_search-keyword",
    "Location": "div.recruiting-search__location",
