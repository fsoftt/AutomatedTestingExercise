    "RemoteOption": "//*[@id=\"jobSearchFilterForm\"]/fieldset/div/p[1]/label",
    "Find": "//form[@id=\"jobSearchFilterForm\"]/descendant::button[@type=\"submit\"]",
    "LatestElement": "//ul[contains(@class, 'search-result__list')]/li[last()]",
    "Apply": "//ul[contains(@class, 'search-result__list')]/li[last()]//a[contains(text(), 'apply')]"
  },
  "ValidateGlobalSearch": {
    "Magnifier": "search-icon",
    "Search": "q",
    "Find": ".//form[@class='header-search__field no-focus']//button",
    "Articles": "//div[@class=\"search-results__items\"]//article",
    "ArticleTitle": "h3",
    "ArticleDescription": "p"
  },
  "Insights": {
    "CarouselNext": "button.slider__right-arrow",
    "InsightArticleTitle": "div.container div.header_and_download",
    "CurrentCarouselTitle": "div.slider div.active div.slider__slide div.text",
    "ReadMoreButton": "div.slider div.active div.slider__slide div.single-slide__cta-container a",
    "CarouselContainer": "div.owl-stage-outer div.owl-stage div.owl-item.active div.slider__slide"
  },
  "Download": {
    "DownloadButton": "//a[@download]"
  }

